<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="GameTableView">
        <attr name="tableCornerRadius" format="dimension"/>
        <attr name="tableType" format="enum">
            <enum name="normal" value="0"/>
            <enum name="rounded" value="1"/>
            <enum name="scallop" value="2"/>
            <enum name="edge" value="3"/>
        </attr>
        <attr name="tableBorderWidth" format="dimension"/>
        <attr name="tableShowBorder" format="boolean"/>
        <attr name="tableBorderColor" format="color"/>
        <attr name="tableBackgroundColor" format="color"/>
        <attr name="tableElevation" format="dimension"/>
        <attr name="android:elevation" />
    </declare-styleable>

</resources>